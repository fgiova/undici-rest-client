{
  "name": "@fgiova/undici-rest-client",
  "version": "2.0.0",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "lint": "eslint",
    "clean": "rimraf dist",
    "build": "tsc",
    "test": "tap",
    "test:debug": "tap --only --timeout=0",
    "test:coverage": "tap --coverage-report=lcovonly --coverage-report=text"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fgiova/undici-rest-client.git"
  },
  "tap": {
    "show-full-coverage": true,
    "exclude": [
      "**/*.d.ts"
    ]
  },
  "keywords": [
    "undici",
    "rest",
    "client",
    "http"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Francesco Giovannini <fgiova@fgiova.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@tsconfig/node20": "^20.1.4",
    "@types/http-errors": "^2.0.4",
    "@types/mime-db": "^1.43.5",
    "@types/node": "^20.17.19",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "eslint": "^9.21.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "fastify": "^5.2.1",
    "semantic-release": "^24.2.3",
    "tap": "^21.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0"
  },
  "dependencies": {
    "http-errors": "^2.0.0",
    "lru-cache": "^11.0.2",
    "mime-db": "^1.53.0",
    "undici": "^7.4.0"
  }
}
